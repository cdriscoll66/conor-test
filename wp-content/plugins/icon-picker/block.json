{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "create-block/icon-picker",
	"version": "0.1.0",
	"title": "Icon Picker",
	"category": "widgets",
	"icon": "marker",
	"description": "Presently a pre selected amount of icons to choose from.",
	"supports": {
		"html": false,
		"align": [ "left", "right", "wide", "full" ]
	},
	"attributes": {
		"icons": {
			"type": "string",
			"source": "attribute",
			"default": "fa-head-side-brain"
		},
		"size": {
			"type": "string",
			"source": "attribute",
			"default": "1x"
		},
		"style": {
			"type": "string",
			"source": "attribute",
			"default": ""
		}
	},
	"textdomain": "icon-picker",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}
